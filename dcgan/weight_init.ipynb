        "def weight_init(m):\n",
        "  classname = m.__class__.__name__\n",
        "  if classname.find('Conv') != -1:\n",
        "    m.weight.data.normal_(0.0, 0.02)\n",
        "  elif classname.find('BatchNorm') != -1:\n",
        "    m.weight.data.normal_(1.0, 0.02)\n",
        "    m.bias.data.fill_(0)  \n",
        "latent_size = 100\n",
        "lr=0.0002\n",
        "betas = (0.5, 0.999)"
      ]
    }
  ]
}